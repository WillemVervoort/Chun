  owner = {Sue},
  shorttitle = {Investigating the climate impacts of global land cover change in the
	community climate system model},
  timestamp = {2012.11.24},
  url = {http://onlinelibrary.wiley.com/store/10.1002/joc.2061/asset/2061_ftp.pdf?v=1&t=h9974unt&s=3efd6bac1e26a21745367cadd9fa9218081447c7}
}

