}

@ARTICLE{Yang1996,
  author = {Yang, Zong-Liang and Dickinson, Robert E.},
  title = {{D}escription of the {B}iosphere-{A}tmosphere {T}ransfer {S}cheme
	({BATS}) for the {S}oil {M}oisture {W}orkshop and evaluation of its
